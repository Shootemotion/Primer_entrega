<!-- lista de productos -->
<table id="productList" class="product-table">
  <thead>
    <tr>
      <th>ID</th>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Stock</th>

    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <td>{{this.id}}</td>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.price}}</td>
      <td>{{this.stock}}</td>
      <td>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>
